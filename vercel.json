{
   "version": 2,
   "builds": [
       {
           "src": "./server3.js",
           "use": "@vercel/node"
       }
   ],
   "routes": [
       {
           "src": "/(.*)",
           "dest": "server3.js"
       }
   ],
   "functions": {
    "server3.js": {
      "includeFiles": "templates/**"
    }
  }
}
